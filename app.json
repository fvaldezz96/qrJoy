{
  "expo": {
    "name": "JoyPark Mobile",
    "slug": "joypark-mobile",
    "scheme": "joypark",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "joyparck",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.jofre.joyparkmobile"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "This app needs access to the camera",
          "microphonePermission": "This app needs access to the microphone"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "a325774b-d375-41e3-930c-d3b1b728c334"
      }
    }
  }
}