{
  "expo": {
    "name": "JoyPark Mobile",
    "slug": "joypark-mobile",
    "scheme": "joypark",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.jofre.joyparkmobile"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "This app needs access to the camera",
          "microphonePermission": "This app needs access to the microphone"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ]
  }
}
